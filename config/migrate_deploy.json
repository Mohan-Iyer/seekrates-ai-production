{
  "deployment_name": "seekrates_ai_panel",
  "target_environment": "production",
  "deploy_target": "fly.io",
  "app_name": "seekrates-ai",
  "region": "ord",
  "exclude": [
    "__pycache__/",
    "*.pyc",
    "*.pyo",
    "*.ipynb",
    "venv/",
    ".env*",
    "api_keys.*",
    "secrets/",
    ".secrets",
    "private_*",
    ".aws/",
    ".gcp/",
    "config/secrets.*",
    "*.broken",
    "*.backup",
    "xxx_*",
    "*.xt",
    "*.md",
    "test*",
    "docs/archives/",
    "docs/docs/",
    "docs/flow_control/",
    "docs/reverse_engineered/",
    "logs/",
    "memory/",
    "notebooks/",
    "output/",
    "outputs/",
    "temp/",
    "templates/"
  ],
  "include_patterns": [
    "src/",
    "static/",
    "app.py",
    "requirements.txt",
    "required_libraries.yaml",
    "setup.sh",
    "setup_env.py",
    "fly.toml",
    "Dockerfile",
    "deployment/agent_deployment_metadata.yaml",
    "deployment/version_manifest.yaml",
    "config/migrate_deploy.json",
    "config/gps_runtime_config.yaml"
  ],
  "exclude_empty_folders": true,
  "deployment_method": "flyctl",
  "health_check_enabled": true,
  "ip_protection_enforced": true
}